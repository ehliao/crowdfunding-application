  },
  "homepage": "https://github.com/min-hinthar/project-3-crowdfunding-app#readme",
  "devDependencies": {
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  }
}
